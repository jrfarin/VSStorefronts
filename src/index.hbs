                                <!-- Accordion Categories -->
                                <div class="accordion mt-5" id="accordionCategoriesDK" data-filter-name="categories">
                                    <div class="vc-card">
                                        <div class="vc-card-header" id="headingCategoriesDK">                                          
                                            <button type="button" class="btn btn-link" data-toggle="collapse" data-target="#collapseCategoriesDK" aria-expanded="true" aria-controls="collapseCategoriesDK">
                                                <span class="FS-15">Categorías</span>
                                                {{!-- <i class="vc-color fas fa-minus"></i> --}}
                                                <span class="fas fa-minus-svg"></span>
                                            </button>									
                                        </div>
                                        <div id="collapseCategoriesDK" class="collapse show" aria-labelledby="headingCategoriesDK" data-parent="#accordionCategoriesDK">
                                            <div class="card-body p-0">
                                                {{> partials/_Categories }}
                                            </div>  
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
