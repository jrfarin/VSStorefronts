    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ShopVenca Storeftonts</title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css">
    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.4.1/css/bootstrap.min.css">
    <!-- Swipe CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/Swiper/5.2.0/css/swiper.min.css">
    <!-- Custom CSS -->
    <link rel="stylesheet" href="../dist/css/main.min.css">  
</head>

<body>  
    <main class="container mt-3 mb-3">
        <article id="vcStorefronts" class="asyncprod">
            
            <!-- Title or Lookbook -->
            <section id="headerTitle" class="row d-none d-lg-block">
                <div class="col-12 d-flex justify-content-center">
                    <h1 class="storefront-title FS-20 text-center text-uppercase font-weight-lighter">Belleza y Cosmética</h1> 
                </div>
                <div class=" col-md-12">
                    <img src="./static/img//banner.jpg" class="img-fluid" >
                    <div class="storefront-description w-50 d-flex justify-content-center">
                        <p class="FS-1 text-center">Descubre nuestra selección en productos de belleza. Fragancias, maquillaje, y todo lo que necesitas para cuidar tus rostro, cuerpo y cabello. También para ellos y los más pequeños. ¡Entra ya!</p>
                    </div>
                </div>
            </section>

            <!-- Commercial Title & Filter CTA -->
            <section id="cTitleFilterCall" class="row d-flex d-lg-none pb-1 m-2">
                <div class="newCommercialTitle FS10">
                    <h1 class="FS-1 font-weight-bold text-secondary">Vestidos de mujer</h1>
                </div>
                <div class="newFilterIcon ml-auto" data-toggle="modal" data-target="#filtersId">
                    <div class="d-flex">
                        <img src="./static/img/filters.svg" alt="Filtros de búsqueda" width="14">
                        <span class="pl-1">Filtros <strong class="vc-color countFilter outmodal">(1)</strong></span>
                    </div>
                    <!-- Modal Filters -->
                    <div class="modal fade" id="filtersId" tabindex="-1" role="dialog" aria-labelledby="filtersModalLabel" aria-hidden="true">
                        <div class="modal-dialog m-0" role="document">
                            <div class="modal-content FS-20vh vc-vw-height-100">
                                <div class="modal-header">
                                    <h5 class="modal-title text-uppercase" id="exampleModalLabel">Filtrar</h5>
                                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                        <span aria-hidden="true"><i class="fas fa-times"></i></span>
                                    </button>
                                </div>                         
                                <div class="modal-body bg-white">
                                    ¨{{> partials/_Filters }}
                                </div>
                            </div>
                        </div>
                    </div>  
                </div>
            </section> 
           
            <!-- Swiper Categories -->
            <section id="categories" class="row mt-2">
                <div class="col-12">
                    <div class="swiper-container swpCategory">
                        <div class="swiper-wrapper font-weight-bold">
                            <a href="/" class="tab swiper-slide">Maquillaje</a>
                            <a href="/" class="tab swiper-slide">Fragancias</a>
                            <a href="/" class="tab swiper-slide">Cuidado facial</a>
                            <a href="/" class="tab swiper-slide">Cuidado del cabello</a>
                            <a href="/" class="tab swiper-slide">Hidratantes</a>
                            <a href="/" class="tab swiper-slide">Ojos y labios</a>
                            <a href="/" class="tab swiper-slide">Exfoliantes</a>
                            <a href="/" class="tab swiper-slide">Salud y bienestar</a>
                            <a href="/" class="tab swiper-slide">Mascarillas</a>
                            <a href="/" class="tab swiper-slide">Coloración</a>
                            <a href="/" class="tab swiper-slide">Reafirmantes</a>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Main  Zone -->
            <section id="products">
                <div id="mainDesktop" class="row">            
                    <!-- Categories & Filters Desktop -->
                    <section id="categoriesAndFiltersDK" class="col-md-4 col-lg-3 mt-3 d-none d-md-block">
                        <div class="row">
                            <div class="col-12 mt-2">
                                <div class="d-flex justify-content-between align-items-center">
                                    <span class="FS-15 text-uppercase">filtros</span>
                                    <span>
                                        <a class="" data-ob-directive="Storefront.FilterClearAllDirective">Borrar filtros</a>
                                    </span>
                                </div>
                                <hr class="w-100 mt-3">

                                <!-- Accordion Filtros -->
